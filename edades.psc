Algoritmo edades
	Definir nom,ape Como Caracter;
	Definir anio,edad Como Entero;
	Escribir "ingrese el nombre ";
	leer nom;
	Escribir "ingrese el apellido ";
	leer ape;
	Escribir "ingrese el anio de nacimiento ";
	leer anio;
	edad<-2022-anio;
	Escribir "el estudiante ",nom," ",ape," tiene ",edad," años";
FinAlgoritmo
